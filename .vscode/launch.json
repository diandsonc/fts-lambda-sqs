{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Launch (web)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/src/FTS.Precatorio.Api/bin/Debug/net5.0/FTS.Precatorio.Api.dll",
      "args": [],
      "cwd": "${workspaceFolder}/src/FTS.Precatorio.Api",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/Views"
      }
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach"
    },
	{
		"name": ".NET Core Launch (LAMBDA SQS)",
		"type": "coreclr",
		"request": "launch",
		"preLaunchTask": "build",
		"program": "${env:USERPROFILE}/.dotnet/tools/dotnet-lambda-test-tool-3.1.exe",
		"args": ["--port", "5001", "--no-ui", "true", "--payload", "create-trade-full"],
		"cwd": "${workspaceFolder}/src/FTS.Precatorio.Message/Lambda.SQSTradeToStepFunction",
		"console": "internalConsole",
		"stopAtEntry": false,
		"internalConsoleOptions": "openOnSessionStart"
	},
	{
		"name": ".NET Core Launch (LAMBDA CreateAssets)",
		"type": "coreclr",
		"request": "launch",
		"preLaunchTask": "build",
		"program": "${env:USERPROFILE}/.dotnet/tools/dotnet-lambda-test-tool-3.1.exe",
		"args": ["--port", "5003"],
		"cwd": "${workspaceFolder}/src/FTS.Precatorio.StepFunction/Lambda.CreateAssets",
		"console": "internalConsole",
		"stopAtEntry": false,
		"internalConsoleOptions": "openOnSessionStart"
	},
	{
		"name": ".NET Core Launch (LAMBDA CreateTicket)",
		"type": "coreclr",
		"request": "launch",
		"preLaunchTask": "build",
		"program": "${env:USERPROFILE}/.dotnet/tools/dotnet-lambda-test-tool-3.1.exe",
		"args": ["--port", "5005"],
		"cwd": "${workspaceFolder}/src/FTS.Precatorio.StepFunction/Lambda.CreateTicket",
		"console": "internalConsole",
		"stopAtEntry": false,
		"internalConsoleOptions": "openOnSessionStart"
	},
	{
		"name": ".NET Core Launch (LAMBDA CreateTrade)",
		"type": "coreclr",
		"request": "launch",
		"preLaunchTask": "build",
		"program": "${env:USERPROFILE}/.dotnet/tools/dotnet-lambda-test-tool-3.1.exe",
		"args": ["--port", "5007"],
		"cwd": "${workspaceFolder}/src/FTS.Precatorio.StepFunction/Lambda.CreateTrade",
		"console": "internalConsole",
		"stopAtEntry": false,
		"internalConsoleOptions": "openOnSessionStart"
	}
  ]
}
