  "version": "0.2.0",
  "configurations": [
    {
      "name": "Lambda (Proxy)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/src/FTS.Precatorio.Api/bin/Debug/net5.0/FTS.Precatorio.Api.dll",
      "args": [],
      "cwd": "${workspaceFolder}/src/FTS.Precatorio.Api",
      "stopAtEntry": false,
      "launchBrowser": {
        "enabled": false
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/Views"
      }
    },
    {
      "name": "Lambda (SQS)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${env:USERPROFILE}/.dotnet/tools/dotnet-lambda-test-tool-3.1.exe",
      "args": ["--port", "5001"],
      "cwd": "${workspaceFolder}/src/FTS.Precatorio.Message/Lambda.SQSTradeToStepFunction",
      "console": "internalConsole",
      "stopAtEntry": false,
      "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "name": "Lambda (CreateAssets)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${env:USERPROFILE}/.dotnet/tools/dotnet-lambda-test-tool-3.1.exe",
      "args": ["--port", "5003"],
      "cwd": "${workspaceFolder}/src/FTS.Precatorio.StepFunction/Lambda.CreateAssets",
      "console": "internalConsole",
      "stopAtEntry": false,
      "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "name": "Lambda (CreateTicket)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${env:USERPROFILE}/.dotnet/tools/dotnet-lambda-test-tool-3.1.exe",
      "args": ["--port", "5005"],
      "cwd": "${workspaceFolder}/src/FTS.Precatorio.StepFunction/Lambda.CreateTicket",
      "console": "internalConsole",
      "stopAtEntry": false,
      "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "name": "Lambda (CreateTrade)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${env:USERPROFILE}/.dotnet/tools/dotnet-lambda-test-tool-3.1.exe",
      "args": ["--port", "5007"],
      "cwd": "${workspaceFolder}/src/FTS.Precatorio.StepFunction/Lambda.CreateTrade",
      "console": "internalConsole",
      "stopAtEntry": false,
      "internalConsoleOptions": "openOnSessionStart"
    }
  ],
  "compounds": [
    {
      "name": "Proxy/SQS/CreateAssets/CreateTicket/CreateTrade",
      "configurations": [
        "Lambda (Proxy)",
        "Lambda (SQS)",
        "Lambda (CreateAssets)",
        "Lambda (CreateTicket)",
        "Lambda (CreateTrade)"
      ]
    }
  ]
}
